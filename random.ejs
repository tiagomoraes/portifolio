<section class="projects" id="idProjects">
  <div class="projects-flex-container">
      <div class="projects-title">
          <span class="projects-title__text">Projects</span>
      </div>
      <div class="projects-slick-container">
          <i class="fas fa-chevron-left projects-slick__arrow" id="projects-slick__left"></i>
          <div class="projects-slick">
          <% projects.forEach(([key, project]) => { %>
              <div class="projects-slick-item">
                  <div class="projects-slick-item__image" style="background-image: url(<%= project.print %>);"></div>
                  <div class="projects-slick-item-content">
                      <span class="projects-slick-item-content__title"><%= project.title %></span>
                      <p class="projects-slick-item-content__text"><%= project.short_description %></p>
                  </div>
                  <div class="projects-slick-item-footer">
                      <a href="/project?pk=<%= key %>" class="projects-slick-item-footer__link">
                          <i class="fas fa-mouse-pointer"></i> Click to see more
                      </a>
                  </div>
              </div>
          <% }); %>
          </div>
          <i class="fas fa-chevron-right projects-slick__arrow" id="projects-slick__right"></i>
      </div>
  </div>
</section>
<section class="contact" id="idContact">
  <div class="contact-filter"></div>
  <div class="contact-flex-container">
      <h2 class="contact-title">CONTACT</h2>
      <form method="POST" action="send" class="contact-form" id="contact-form">
          <div class="contact-form-box">
              <input type="text" name="name" id="name" placeholder="Name*" class="contact-form__field form-small" required>
              <input type="tel" name="phone" id="phone" placeholder="Phone" class="contact-form__field form-small">
          </div>
          <input type="email" name="email" id="email" placeholder="Email*" class="contact-form__field" required>
          <input type="text" name="subject" id="subject" placeholder="Subject" class="contact-form__field">
          <div class="contact-form__field">
              <select name="how_select" id="how" class="contact-form__selector">
                  <option value="default" disabled selected>How did you get here?</option>
                  <option value="Social Network">Social Network</option>
                  <option value="CIn Website">Cin Website</option>
                  <option value="Some friend">Some Friend</option>
                  <option value="Other">Other</option>                            
              </select>
          </div>
          <textarea type="text" name="message" id="message" placeholder="Message" class="contact-form__field form-tall"></textarea>
          <button type="submit" class="contact-form__submit">Send</button>
      </form>
  </div>
</section>
